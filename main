    def Match(url):
        try:
            text = requests.get(url).text
            soup = bs4.BeautifulSoup(text, 'lxml')
            result = set()
            for word in words:
                result.update(re.findall(word, text))
            return list(result)
        except Exception as ex:
            return [str(ex)]
        finally:
            print('', end='', flush=True)

